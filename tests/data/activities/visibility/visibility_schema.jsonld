{
    "@context": "https://raw.githubusercontent.com/ReproNim/reproschema/1.0.0-rc4/contexts/generic",
    "@type": "reproschema:Activity",
    "@id": "visibility_schema.jsonld",
    "schemaVersion": "1.0.0-rc4",
    "version": "0.0.1",
    "prefLabel": {
        "en": "Visibility"
    },
    "description": "visibility",
    "preamble": {
        "en": ""
    },
    "ui": {
        "shuffle": false,
        "order": [
            "items/base.jsonld",
            "items/base1.jsonld",
            "items/base0.jsonld",
            "items/radio_vis.jsonld",
            "items/multi_gt_5.jsonld",
            "items/multi_lt_2.jsonld",
            "items/multi_2_or_5.jsonld",
            "items/select_boolean.jsonld",
            "items/select_boolean1.jsonld",
            "items/select_boolean0.jsonld"
        ],
        "addProperties": [
            {
                "variableName": "base",
                "isAbout": "items/base.jsonld",
                "prefLabel": {
                    "en": "base"
                },
                "isVis": true,
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "base1",
                "isAbout": "items/base1.jsonld",
                "prefLabel": {
                    "en": "base==1"
                },
                "isVis": "base==1",
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "base0",
                "isAbout": "items/base0.jsonld",
                "prefLabel": {
                    "en": "base==0"
                },
                "isVis": "base==0",
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "radio_vis",
                "isAbout": "items/radio_vis.jsonld",
                "prefLabel": {
                    "en": "radio vis"
                },
                "isVis": true,
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "multi_gt_5",
                "isAbout": "items/multi_gt_5.jsonld",
                "prefLabel": {
                    "en": "multi gt 5"
                },
                "isVis": "radio_vis>5",
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "multi_lt_2",
                "isAbout": "items/multi_lt_2.jsonld",
                "prefLabel": {
                    "en": "multi lt 2"
                },
                "isVis": "radio_vis<=2",
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "multi_2_or_5",
                "isAbout": "items/multi_2_or_5.jsonld",
                "prefLabel": {
                    "en": "multi 2 or 5"
                },
                "isVis": "radio_vis == 2 OR radio_vis == 5",
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "select_boolean",
                "isAbout": "items/select_boolean.jsonld",
                "prefLabel": {
                    "en": "select boolean"
                },
                "isVis": true,
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "select_boolean1",
                "isAbout": "items/select_boolean1.jsonld",
                "prefLabel": {
                    "en": "select boolean==1"
                },
                "isVis": "select_boolean==1",
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            },
            {
                "variableName": "select_boolean0",
                "isAbout": "items/select_boolean0.jsonld",
                "prefLabel": {
                    "en": "select boolean==0"
                },
                "isVis": "select_boolean==0",
                "requiredValue": false,
                "allow": [
                    "reproschema:Skipped"
                ]
            }
        ],
        "allow": [
            "reproschema:AutoAdvance",
            "reproschema:AllowExport"
        ]
    },
    "messages": [
        {
            "message": "Radio vis must be greater than Foo.",
            "jsExpression": "multi_2_or_5 < radio_vis"
        }
    ]
}